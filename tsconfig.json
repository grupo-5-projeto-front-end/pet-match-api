{
    "compilerOptions": {
      "target": "es6",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "./dist", //deploy
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "typeRoots": ["src/@types", "node_modules/@types"]
    }
  }
  